<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="X_Forms.NavigationBsp.FlyoutBsp.FlyoutP"
             xmlns:pages="clr-namespace:X_Forms.NavigationBsp.FlyoutBsp"
            xmlns:main="clr-namespace:X_Forms">
  <FlyoutPage.Flyout>
    <pages:FlyoutPFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <main:MainPage/>
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>